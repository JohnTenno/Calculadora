{
  "name": "calculadora",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start:dev": "ng serve --o",
    "serve": "ng serve --o --port 4200",
    "deploy:dev": "npm run build:firebase && npm run deploy:firebase:dev",
    "deploy:prod": "npm run build:firebase && npm run deploy:firebase:prod",
    "deploy:firebase:dev": "firebase deploy --only hosting ",
    "deploy:firebase:prod": "firebase deploy --only -P production",
    "build": "npm run build:ng:dev",
    "build:firebase": " npm run build:ng:firebase:prod",
    "build:staging": " npm run build:ng:staging",
    "build:prod": " npm run build:ng:prod",
    "build:ng:dev": "ng build --output-path=./public",
    "build:ng:staging": "ng build --output-path=./public --configuration=production",
    "build:ng:prod": "ng build --output-path=./public --configuration=production",
    "build:ng:firebase:prod": "ng build --output-path=./public --configuration=production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.8",
    "@angular/cdk": "^17.0.4",
    "@angular/common": "^17.0.8",
    "@angular/compiler": "^17.0.8",
    "@angular/core": "^17.0.8",
    "@angular/fire": "^17.0.0",
    "@angular/forms": "^17.0.8",
    "@angular/localize": "^17.3.11",
    "@angular/material": "^17.0.4",
    "@angular/platform-browser": "^17.0.8",
    "@angular/platform-browser-dynamic": "^17.0.8",
    "@angular/router": "^17.0.8",
    "@auth0/angular-jwt": "^5.1.2",
    "@fullcalendar/angular": "^6.1.8",
    "@fullcalendar/core": "^6.1.8",
    "@fullcalendar/daygrid": "^6.1.8",
    "@fullcalendar/interaction": "^6.1.8",
    "@fullcalendar/list": "^6.1.8",
    "@fullcalendar/timegrid": "^6.1.8",
    "@fullcalendar/web-component": "^6.1.8",
    "@popperjs/core": "^2.11.8",
    "@tabler/icons-webfont": "^2.12.0",
    "@types/chart.js": "^2.9.37",
    "angular-imask": "^7.0.1-alpha.1",
    "bootstrap": "^5.2.3",
    "chart.js": "^3.8.0",
    "chartjs-plugin-annotation": "^1.4.0",
    "font-awesome": "^4.7.0",
    "fullcalendar": "^6.1.8",
    "jquery": "^3.6.0",
    "ngx-toastr": "^18.0.0",
    "primeicons": "^6.0.1",
    "primeng": "^17.3.1",
    "quill": "^1.3.7",
    "rxjs": "^7.5.6",
    "tslib": "^2.3.0",
    "uuid": "^9.0.1",
    "vrchat": "^1.17.5",
    "xlsx": "^0.18.5",
    "zone.js": "^0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.9",
    "@angular/cli": "~17.0.9",
    "@angular/compiler-cli": "^17.0.8",
    "@types/jasmine": "~4.0.0",
    "@types/uuid": "^9.0.7",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "^5.2.2"
  }
}
